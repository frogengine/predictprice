<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" version="2.1">
    <persistence-unit name="stock_prediction">
        <class>com.ysn.predictprice.entities.StockPrediction</class>
        <properties>
            <!-- mandatory -->
            <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.user" value="predictpriceadmin"/>
            <property name="javax.persistence.jdbc.password" value="Hello1234!"/>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/predictprice"/>
            <property name="hibernate.dialect" value="org.hibernate.dialect.MySQL5InnoDBDialect"/>
        </properties>
    </persistence-unit>
</persistence>